{
  "extends": ["@tsconfig/recommended/tsconfig.json", "@tsconfig/node-lts/tsconfig.json", "@tsconfig/strictest/tsconfig"],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noPropertyAccessFromIndexSignature": false,
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": false,
    "noImplicitOverride": false
  }
}
