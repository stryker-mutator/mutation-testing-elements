{
  "name": "mutation-testing-real-time",
  "version": "3.7.2",
  "description": "Helpers to create a NodeJS server for real-time reporting for a mutation testing elements report",
  "module": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js"
  },
  "type": "module",
  "nx": {
    "name": "real-time"
  },
  "keywords": [
    "mutation",
    "testing",
    "real-time",
    "middleware"
  ],
  "scripts": {
    "test": "mocha --node-option enable-source-maps --forbid-only --forbid-pending --retries 2 dist/test/**/*.js",
    "lint": "eslint .",
    "stryker": "stryker run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/mutation-testing-elements.git",
    "directory": "packages/real-time"
  },
  "homepage": "https://github.com/stryker-mutator/mutation-testing-elements/tree/master/packages/real-time#readme",
  "devDependencies": {
    "@stryker-mutator/core": "catalog:",
    "@stryker-mutator/mocha-runner": "catalog:",
    "@stryker-mutator/typescript-checker": "catalog:",
    "eslint": "catalog:",
    "eslint-config-mte": "workspace:*",
    "eventsource": "4.1.0",
    "mutation-testing-report-schema": "workspace:*",
    "rxjs": "7.8.2"
  }
}
