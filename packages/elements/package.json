{
  "name": "mutation-testing-elements",
  "version": "2.0.3",
  "description": "A suite of web components for a mutation testing report.",
  "unpkg": "dist/mutation-test-elements.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "karma start --single-run",
    "test:unit:debug": "karma start --browsers ChromeHeadlessDebug",
    "test:integration": "mocha --config ./test/integration/.mocharc.cjs",
    "test:integration:update": "cross-env UPDATE_ALL_SCREENSHOTS=true HEADLESS=true mocha --config ./test/integration/.mocharc.cjs",
    "test:integration:headless": "cross-env HEADLESS=true mocha --config ./test/integration/.mocharc.cjs",
    "postpublish": "PUBLISH_ELEMENTS=true ../metrics-scala/npmProjPublish.sh",
    "stryker": "cross-env STRYKER=true stryker run",
    "lint": "eslint --ext .js,.ts ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stryker-mutator/mutation-testing-elements.git",
    "directory": "packages/elements"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/mutation-testing-elements/issues"
  },
  "homepage": "https://github.com/stryker-mutator/mutation-testing-elements/tree/master/packages/elements#readme",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/express": "^4.17.0",
    "@types/prismjs": "^1.16.1",
    "@types/resemblejs": "^4.1.0",
    "@types/selenium-webdriver": "^4.0.11",
    "@types/webpack-env": "^1.13.9",
    "autoprefixer": "^10.4.13",
    "canvas": "^2.11.0",
    "chromedriver": "^115.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^6.0.0",
    "cssnano": "^6.0.0",
    "eslint-config-mte": "*",
    "express": "^4.17.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "lit": "^2.0.2",
    "mutation-testing-metrics": "2.0.3",
    "mutation-testing-report-schema": "2.0.3",
    "postcss": "^8.2.13",
    "postcss-loader": "^7.0.0",
    "prismjs": "^1.21.0",
    "resemblejs": "^5.0.0",
    "rxjs": "^7.0.0",
    "sass": "^1.22.3",
    "sass-loader": "^13.0.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
