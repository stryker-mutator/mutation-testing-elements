{
  "name": "root",
  "private": true,
  "version": "0.0.6",
  "description": "A suite of web components for a mutation testing report.",
  "main": "dist/mutation-test-elements.js",
  "scripts": {
    "clean": "rimraf --glob packages/*/{dist,dist-test,src-generated}",
    "all": "npm run clean && npm run build && npm run lint && npm run test",
    "postinstall": "lerna bootstrap",
    "lint": "npm run lint:json && npm run lint:ts",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:json": "prettier --check .",
    "prebuild": "npm run generate",
    "generate": "lerna run generate",
    "build": "tsc -b && lerna run build",
    "test": "lerna run test --stream --concurrency 1",
    "start": "lerna run start --stream --no-prefix",
    "start:tsc": "tsc --build --watch",
    "download-incremental-reports": "bash tasks/download-incremental-reports.sh",
    "lerna:version:patch": "lerna version patch",
    "lerna:version:minor": "lerna version minor",
    "lerna:version:major": "lerna version major",
    "lerna:publish": "lerna publish from-git --yes --no-verify-access"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/mutation-testing-elements.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/mutation-testing-elements/issues"
  },
  "homepage": "https://github.com/stryker-mutator/mutation-testing-elements#readme",
  "devDependencies": {
    "@stryker-mutator/core": "^6.4.1",
    "@stryker-mutator/karma-runner": "^6.4.1",
    "@stryker-mutator/mocha-runner": "^6.4.1",
    "@stryker-mutator/typescript-checker": "^6.4.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.0",
    "@types/resemblejs": "^4.1.0",
    "@types/selenium-webdriver": "^4.0.11",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.2",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "ajv": "^8.6.3",
    "autoprefixer": "^10.4.13",
    "canvas": "^2.11.0",
    "chai": "^4.2.0",
    "chromedriver": "^109.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^6.0.0",
    "cssnano": "^5.0.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "~8.7.0",
    "eslint-plugin-import": "~2.27.4",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "express": "^4.17.1",
    "json-schema-to-typescript": "^12.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "lerna": "^6.0.0",
    "lit": "^2.0.2",
    "mocha": "^10.0.0",
    "postcss": "^8.2.13",
    "postcss-loader": "^7.0.0",
    "prettier": "~2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "resemblejs": "^4.0.0",
    "rimraf": "^4.4.0",
    "rxjs": "^7.0.0",
    "sass": "^1.22.3",
    "sass-loader": "^13.0.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "tailwindcss": "^3.2.1",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.7.3"
  }
}
