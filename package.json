{
  "name": "root",
  "private": true,
  "version": "0.0.6",
  "description": "A suite of web components for a mutation testing report.",
  "main": "dist/mutation-test-elements.js",
  "scripts": {
    "clean": "rimraf packages/*/{dist,dist-test,src-generated}",
    "all": "npm run clean && npm run build && npm run lint && npm run test",
    "postinstall": "lerna bootstrap && link-parent-bin",
    "lint": "npm run lint:json && npm run lint:ts",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:json": "prettier --check *.json",
    "prebuild": "npm run generate",
    "generate": "lerna run generate",
    "build": "tsc -b && lerna run build",
    "test": "lerna run test --stream",
    "start": "webpack-dev-server --open",
    "start:tsc": "tsc --build --watch",
    "lerna:version:patch": "lerna version --conventional-commits patch",
    "lerna:version:minor": "lerna version --conventional-commits minor",
    "lerna:version:major": "lerna version --conventional-commits major",
    "lerna:publish": "lerna publish from-git --yes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stryker-mutator/mutation-testing-elements.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stryker-mutator/mutation-testing-elements/issues"
  },
  "homepage": "https://github.com/stryker-mutator/mutation-testing-elements#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@stryker-mutator/core": "^4.1.0",
    "@stryker-mutator/karma-runner": "^4.1.0",
    "@stryker-mutator/mocha-runner": "^4.1.0",
    "@stryker-mutator/typescript-checker": "^4.1.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.11",
    "@types/resemblejs": "^3.2.0",
    "@types/selenium-webdriver": "^4.0.11",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.2",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "ajv": "^6.10.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chromedriver": "^88.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-prettier": "~3.3.0",
    "express": "^4.17.1",
    "json-schema-to-typescript": "^10.0.2",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "link-parent-bin": "^2.0.0",
    "lit-element": "^2.2.0",
    "mocha": "^8.2.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "~2.2.0",
    "resemblejs": "^3.2.5",
    "rimraf": "^3.0.0",
    "rxjs": "^6.6.3",
    "sass": "^1.22.3",
    "sass-loader": "^11.0.1",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.12",
    "ts-loader": "^8.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.24.3",
